# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
  email: String!
  age: Int!
  work: String!
}

type Query {
  user(
    id: Int!
  ): User
}

input NewUser {
  name: String!
  email: String
  age: Int
  work: String
}

type Mutation {
  createUser(input: NewUser!): User!
}
